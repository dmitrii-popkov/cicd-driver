<settings>
    <mirrors>
        <mirror>
            <id>central-proxy</id>
            <name>Local proxy of central repo</name>
            <url>https://artifactory.infotecs.ru/artifactory/maven-central/</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>

    <servers>
        <server>
            <id>github</id>
            <username>${repo.user}</username>
            <password>${repo.pwd}</password>
        </server>
    </servers>

    <profiles>

    </profiles>
</settings>